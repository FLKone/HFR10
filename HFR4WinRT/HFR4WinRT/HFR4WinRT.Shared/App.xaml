<Application x:Class="HFR4WinRT.App" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:local="using:HFR4WinRT">
  <Application.Resources>
      <ResourceDictionary>
          <ResourceDictionary.MergedDictionaries>
              <ResourceDictionary Source="Resources/Colors.xaml"/>
              <ResourceDictionary Source="Resources/Converters.xaml"/>
              <ResourceDictionary Source="Resources/Styles.xaml"/>
              <ResourceDictionary Source="Resources/Templates.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <vm:Locator x:Key="Locator"
                        xmlns:vm="using:HFR4WinRT.ViewModel" />
        </ResourceDictionary>
  </Application.Resources>
</Application>